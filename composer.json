{
    "name": "phpactor/logging-extension",
    "description": "PSR logging extension",
    "license": "MIT",
    "type": "phpactor-extension",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "monolog/monolog": "^1.23",
        "phpactor/container": "^1.1"
    },
    "require-dev": {
        "phpstan/phpstan": "~0.12.0",
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "~2.15.0"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\Extension\\Logger\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Extension\\Logger\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "integrate": [
            "./vendor/bin/phpunit",
            "./vendor/bin/phpstan analyze",
            "./vendor/bin/php-cs-fixer fix"
        ]
    },
    "extra": {
        "phpactor.extension_class": "Phpactor\\Extension\\Logger\\LoggingExtension",
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}